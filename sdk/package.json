    },
    "./react/passkey-auth-menu2/skeleton": {
      "import": "./dist/esm/react/components/PasskeyAuthMenu/skeleton.js",
      "default": "./dist/esm/react/components/PasskeyAuthMenu/skeleton.js",
      "types": "./dist/types/client/src/react/components/PasskeyAuthMenu/skeleton.d.ts"
    },
    "./react/passkey-auth-menu2/preload": {
      "import": "./dist/esm/react/components/PasskeyAuthMenu/preload.js",
      "default": "./dist/esm/react/components/PasskeyAuthMenu/preload.js",
      "types": "./dist/types/client/src/react/components/PasskeyAuthMenu/preload.d.ts"
    },
    "./react/provider": {
      "import": "./dist/esm/react/context/TatchiPasskeyProvider.js",
      "default": "./dist/esm/react/context/TatchiPasskeyProvider.js",
      "types": "./dist/types/client/src/react/context/TatchiPasskeyProvider.d.ts"
    },
    "./react/styles": "./dist/esm/react/styles/styles.css",
    "./chainsigs": {
      "import": "./dist/esm/chainsigs/index.js",
      "default": "./dist/esm/chainsigs/index.js",
      "types": "./dist/types/client/src/chainsigs/index.d.ts"
    },
    "./WalletIframe/client/html": {
      "import": "./dist/esm/core/WalletIframe/client/html.js",
      "default": "./dist/esm/core/WalletIframe/client/html.js",
      "types": "./dist/types/client/src/core/WalletIframe/client/html.d.ts"
    },
    "./worker": "./dist/web3authn-signer.worker.js",
    "./wasm": "./dist/wasm_signer_worker_bg.wasm",
    "./wasm-js": "./dist/wasm_signer_worker.js",
    "./components/modal": {
      "import": "./dist/esm/core/WebAuthnManager/Components/modal.js",
      "default": "./dist/esm/core/WebAuthnManager/Components/modal.js",
      "types": "./dist/types/client/src/core/WebAuthnManager/Components/modal.d.ts"
    },
    "./components/embedded": {
      "import": "./dist/esm/core/WebAuthnManager/Components/embedded.js",
