    },
    "./react/passkey-auth-menu2": {
      "import": "./dist/esm/react/components/PasskeyAuthMenu/shell.js",
      "default": "./dist/esm/react/components/PasskeyAuthMenu/shell.js",
      "types": "./dist/types/client/src/react/components/PasskeyAuthMenu/shell.d.ts"
    },
    "./react/passkey-auth-menu2/client": {
      "import": "./dist/esm/react/components/PasskeyAuthMenu/client.js",
      "default": "./dist/esm/react/components/PasskeyAuthMenu/client.js",
