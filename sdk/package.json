  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/types/client/src/react/index.d.ts"
      ],
      "react/*": [
        "dist/types/client/src/react/*"
      ]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "./scripts/build-dev.sh",
    "build:prod": "./scripts/build-prod.sh",
    "build:sri-tarball": "bash ./scripts/build-sri-tarball.sh",
    "build:rolldown": "rolldown -c rolldown.config.ts",
    "dev": "rolldown -c rolldown.config.ts -w",
    "test": "npm run build:check:fresh || npm run build && USE_RELAY_SERVER=1 playwright test --reporter=line",
    "test:lite": "npm run build:check:fresh || npm run build && USE_RELAY_SERVER=1 playwright test -c playwright.lite.config.ts --reporter=line",
    "test:inline": "npm run build:check:fresh || npm run build && USE_RELAY_SERVER=1 playwright test --reporter=line",
    "test:e2e": "playwright test ../tests/e2e --reporter=line",
    "test:unit": "npm run build:check:fresh || npm run build && playwright test ../tests/unit --reporter=line",
    "test:threshold-core": "playwright test -c playwright.relayer.config.ts ../tests/relayer/threshold-ecdsa.signature-harness.test.ts ../tests/relayer/threshold-ecdsa.durable-stores.test.ts --reporter=line && playwright test ../tests/unit/thresholdEcdsa.tempoHighLevel.unit.test.ts --reporter=line",
    "smoke:eth-signer:node-runtime": "node ./scripts/smoke-eth-signer-node-runtime.mjs",
    "smoke:eth-signer:workers-runtime": "node ./scripts/smoke-eth-signer-workers-runtime.mjs",
    "smoke:eth-signer:runtimes": "pnpm smoke:eth-signer:node-runtime && pnpm smoke:eth-signer:workers-runtime",
    "test:wallet-iframe": "USE_RELAY_SERVER=0 playwright test ../tests/wallet-iframe --reporter=line",
    "test:lit-components": "USE_RELAY_SERVER=0 playwright test ../tests/lit-components --reporter=line",
    "test:relayer": "playwright test -c playwright.relayer.config.ts --reporter=line",
    "test:relay-email-recovery-zk": "node ../tests/scripts/test-relay-email-recovery-zk.mjs",
    "build:check:fresh": "./scripts/check-build-freshness.sh",
    "size:lite": "node ./scripts/report-lite-bundle-sizes.mjs",
    "size:lite:check": "node ./scripts/report-lite-bundle-sizes.mjs --check",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "NEAR",
    "passkey",
    "webauthn",
    "authentication",
    "blockchain",
    "react",
    "typescript",
    "sdk"
  ],
  "author": "web3authn.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/web3-authn/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/web3-authn/sdk/issues"
  },
  "homepage": "https://github.com/web3-authn",
  "peerDependencies": {
    "express": ">=4.17.0",
    "jsqr": ">=1.4.0",
    "qrcode": ">=1.3.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "qrcode": {
      "optional": true
    },
    "jsqr": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "dependencies": {
    "@lit/react": "^1.0.8",
    "@noble/ciphers": "^2.1.1",
    "@noble/curves": "^2.0.1",
    "@noble/ed25519": "^2.1.0",
    "@noble/hashes": "^2.0.1",
    "@simplewebauthn/server": "^13.2.2",
    "bs58": "^6.0.0",
    "idb": "^8.0.3",
    "lit": "^3.3.1",
    "pg": "^8.17.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@near-js/types": "^2.4.0",
    "@playwright/test": "^1.40.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "borsh": "^2.0.0",
    "express": "^4.21.2",
    "js-sha256": "^0.11.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rolldown": "^1.0.0-beta.27",
    "rollup-plugin-css-only": "^4.5.2",
    "typescript": "^5.8.3"
  }
}
